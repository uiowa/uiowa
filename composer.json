{
    "name": "acquia/blt-project",
    "description": "Project template for Drupal 8 sites built with BLT.",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "GPL-2.0-only",
    "repositories": {
        "block_content_template": {
            "type": "git",
            "url": "git@github.com:uiowa/block_content_template.git"
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "easysvg": {
            "type": "git",
            "url": "git@github.com:joewhitsitt/easysvg"
        },
        "uiowa_bar": {
            "type": "git",
            "url": "git@github.com:uiowa/uiowa_bar"
        },
        "uiowa_auth": {
            "type": "git",
            "url": "git@github.com:uiowa/uiowa_auth"
        },
        "uiowa_footer": {
            "type": "git",
            "url": "git@github.com:uiowa/uiowa_footer"
        },
        "colorbox":{
            "type": "package",
            "package": {
                "name": "jackmoore/colorbox",
                "version": "1.6.4",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/jackmoore/colorbox/archive/1.6.4.zip",
                    "type": "zip"
                }
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Uiowa\\": "blt/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Uiowa\\Tests\\PHPUnit\\": "tests/phpunit/src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "acquia/blt": "^11.0.1",
        "acquia/memcache-settings": "*",
        "bower-asset/chosen": "^1.8",
        "bower-asset/fontawesome": "^5.14",
        "bower-asset/lazysizes": "^5.2",
        "drupal-composer/drupal-scaffold": "^2.5.4",
        "drupal/acquia_connector": "^1.5.0",
        "drupal/acquia_purge": "^1.0",
        "drupal/address": "^1.7",
        "drupal/administerusersbyrole": "^3.0",
        "drupal/allowed_formats": "^1.1",
        "drupal/better_normalizers": "^1.0@beta",
        "drupal/big_pipe_sessionless": "^1.2",
        "drupal/block_content_permissions": "^1.9",
        "drupal/colorbox": "^1.4",
        "drupal/components": "^1.0",
        "drupal/config_ignore": "^2.2.0",
        "drupal/config_inspector": "^1.0",
        "drupal/config_split": "^1.4",
        "drupal/core": "^8.9.3",
        "drupal/cshs": "^1.0@beta",
        "drupal/date_recur_modular": "^2.0",
        "drupal/default_content": "^1.0@alpha",
        "drupal/devel": "^2.1",
        "drupal/diff": "^1.0@RC",
        "drupal/draggableviews": "^1.2",
        "drupal/editor_advanced_link": "^1.4",
        "drupal/entity_browser": "^2.4.0",
        "drupal/entity_embed": "^1.0@RC",
        "drupal/entity_reference_revisions": "^1.5",
        "drupal/entity_reference_unpublished": "^1.1",
        "drupal/entity_usage": "^2.0@alpha",
        "drupal/features": "^3.8",
        "drupal/field_group": "^3.0",
        "drupal/file_mdm": "^2.1",
        "drupal/focal_point": "^1.5",
        "drupal/fontawesome": "^2.9",
        "drupal/fragments": "^1.0@beta",
        "drupal/google_analytics": "^3.0",
        "drupal/google_tag": "^1.3",
        "drupal/heading": "^1.4",
        "drupal/honeypot": "^2.0",
        "drupal/imagecache_external": "^1.0",
        "drupal/imagemagick": "^3.0",
        "drupal/inline_entity_form": "^1.0.0-rc2",
        "drupal/layout_builder_lock": "^1.0@RC",
        "drupal/layout_builder_operation_link": "^1.0",
        "drupal/layout_builder_restrictions": "^2.6",
        "drupal/layout_builder_styles": "^1.0@beta",
        "drupal/lazy": "^3.4",
        "drupal/lb_direct_add": "1.x-dev",
        "drupal/linkit": "^5.0",
        "drupal/linkit_media_library": "^1.0",
        "drupal/mailsystem": "^4.1",
        "drupal/masquerade": "^2.0@beta",
        "drupal/media_entity_facebook": "^2.0",
        "drupal/media_entity_instagram": "3.x-dev",
        "drupal/media_entity_twitter": "^2.4",
        "drupal/media_library_theme_reset": "^1.0",
        "drupal/menu_admin_per_menu": "^1.0",
        "drupal/menu_block": "1.x-dev#fbfb8fe",
        "drupal/menu_breadcrumb": "^1.7",
        "drupal/menu_link_weight": "^1.0@beta",
        "drupal/metatag": "1.x-dev",
        "drupal/migrate_plus": "^4.2",
        "drupal/migrate_tools": "^4.3",
        "drupal/mimemail": "^1.0@alpha",
        "drupal/node_revision_delete": "^1.0@beta",
        "drupal/override_node_options": "^2.4",
        "drupal/paragraphs": "^1.10",
        "drupal/pathauto": "^1.6",
        "drupal/purge": "^3.0@beta",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/redirect": "1.x-dev",
        "drupal/reroute_email": "^1.1",
        "drupal/robotstxt": "^1.2",
        "drupal/schema_metatag": "^1.2",
        "drupal/search_api_autocomplete": "^1.2",
        "drupal/simple_sitemap": "^3.5",
        "drupal/simplei": "^1.1",
        "drupal/siteimprove": "^1.5",
        "drupal/smart_trim": "^1.1",
        "drupal/stage_file_proxy": "^1.0",
        "drupal/superfish": "^1.2",
        "drupal/swiftmailer": "^1.0@beta",
        "drupal/twig_tweak": "^2.4",
        "drupal/upgrade_status": "^2.0",
        "drupal/views_ical": "^1.0@alpha",
        "drupal/views_tree": "^2.0@alpha",
        "drupal/webform": "^5.12",
        "drupal/workbench_email": "^1.2",
        "drush/drush": "^10",
        "jackmoore/colorbox": "^1.6",
        "kartsims/easysvg": "dev-master",
        "npm-asset/fontawesome-iconpicker": "^3.2",
        "npm-asset/jquery-collapse": "^1.1",
        "npm-asset/moment": "^2.24",
        "npm-asset/slick-carousel": "^1.8",
        "richardbporter/drush-users-commands": "^2.0",
        "uiowa/block_content_template": "dev-master",
        "uiowa/uiowa_auth": "^1.0",
        "uiowa/uiowa_bar": "^1.0.0",
        "uiowa/uiowa_footer": "^1.0"
    },
    "require-dev": {
        "acquia/blt-require-dev": "11.x-dev",
        "drupal/core-dev": "^8.8",
        "geerlingguy/drupal-vm": "~6.0",
        "typhonius/acquia-php-sdk-v2": "^2.0.0",
        "zaporylie/composer-drupal-optimizations": "^1.1"
    },
    "extra": {
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/inline_entity_form": {
                "Custom blocks created in inline forms should not be reusable": "https://www.drupal.org/files/issues/2020-05-04/3002175-26.patch"
            },
            "drupal/menu_block": {
                "Add option to make the starting level follow the active menu item": "https://www.drupal.org/files/issues/2018-05-01/menu_block-follow-2756675-43.patch",
                "Hide block title if menu tree contains no links": "https://www.drupal.org/files/issues/2018-05-08/menu_block-hide_block_if_no_links-2757215-9.patch",
                "Suppress display of regular system menu blocks in Layout Builder": "https://www.drupal.org/files/issues/2019-12-11/menu_block-remove_duplicates-3062278-11.patch"
            },
            "drupal/metatag": {
                "Allow each tag to have its own permission": "https://www.drupal.org/files/issues/metatag-n2782797-16.patch"
            },
            "drupal/schema_metatag": {
                "Webpage > SearchAction missing target": "https://www.drupal.org/files/issues/2020-02-17/3055278-7.patch"
            },
            "drupal/default_content": {
                "Respect revision ids for entity_reference_revisions": "https://www.drupal.org/files/issues/2018-08-06/exported_revision_ids-2989887-6.patch"
            },
            "drupal/purge": {
                "Port purge commands to Drush 9": "https://www.drupal.org/files/issues/2921309-17-port-purge-drush-9.patch"
            },
            "drupal/config_ignore": {
                "Support for export filtering via Drush": "https://www.drupal.org/files/issues/2018-10-14/support-for-export-2857247-24.patch"
            },
            "drupal/core": {
                "The Media library doesn't modify the 'media' view if installed in a profile": "https://www.drupal.org/files/issues/2019-05-12/2992056.patch",
                "Site extensions don't get rediscovered after drush cr": "https://www.drupal.org/files/issues/2018-07-11/specify_sitepath_cache_rebuild_extension_discovery-2985199-3.patch",
                "Allow alter oembed providers": "https://www.drupal.org/files/issues/2019-05-15/drupal-allow_alter_oembed_providers-3008119-4.patch",
                "save edits to media embeds": "https://www.drupal.org/files/issues/2019-12-19/3102249-fix-media-ckeditor-snapshop.patch",
                "add img dimensions": "https://www.drupal.org/files/issues/2019-11-19/responsive_image-provide_dimensions_for_image_tag-3095126-4.patch",
                "fix duplicate drag handles in claro": "https://www.drupal.org/files/issues/2020-05-28/3092181-87-89x.patch",
                "[PP-1] Reverting entity revisions that contain custom blocks erroneously triggers EntityChangedConstraint": "https://www.drupal.org/files/issues/2019-05-13/3053881-11.patch",
                "Hidden buttons in off-canvas dialog are not being hidden": "https://www.drupal.org/files/issues/2020-02-28/3070375-4.patch",
                "Off-canvas style resets are overriding styles (especially SVGs) in Safari resulting in display issues": "https://www.drupal.org/files/issues/2019-05-07/off-canvas-style-resets-2958588-13.patch",
                "Media viewmode control": "https://www.drupal.org/files/issues/2020-01-31/3097416-ckeditor-view-modes-8.9-combined.patch"
            },
            "drupal/layout_builder_styles": {
                "Section dependency on layout plugin configuration form": "https://www.drupal.org/files/issues/2019-08-23/layout_builder_styles-3062261-%2310.patch",
                "Grouping of layout builder styles": "https://www.drupal.org/files/issues/2020-06-22/style_groups_for_layout_builder_styles-3075502--beta2.patch",
                "Block theme suggestions not working for layout builder blocks using styles": "https://www.drupal.org/files/issues/2020-04-10/block-theme-suggestions-not-working-for-layout-builder-blocks-using-styles-3126609_0.patch"
            },
            "drupal/redirect": {
                "Match redirect": "https://www.drupal.org/files/issues/2020-04-13/redirect-n2831605-31.patch"
            },
            "drupal/entity_embed": {
                "Logs flooded with warning messages Invalid display settings encountered": "https://www.drupal.org/files/issues/2019-12-11/3077225-10.reduce-invalid-config-logs.patch"
            },
            "drupal/media_entity_facebook": {
                "Allow creation of new Facebook media entities using the Media Browser": "https://www.drupal.org/files/issues/2020-03-31/3120085-media-library-add-5.patch"
            }
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/modules/uiowa/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "initial": {
                "sites/default/default.services.yml": "sites/default/services.yml",
                "sites/default/default.settings.php": "sites/default/settings.php"
            },
            "excludes": [
                "robots.txt"
            ]
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "blt internal:create-project:init-repo"
        ],
        "post-drupal-scaffold-cmd": [
            "patch -p1 < patches/core_htaccess.patch"
        ],
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "nuke": [
            "rm -rf vendor composer.lock docroot/core docroot/modules/contrib docroot/profiles/contrib docroot/themes/contrib",
            "@composer clearcache --ansi",
            "@composer install --ansi"
        ]
    }
}
