        "drupal/ui_icons": "^1.0@beta",
        "drupal/views_bulk_operations": "^4.3",
        "drupal/views_custom_cache_tag": "^1.3",
        "drupal/views_data_export": "^1.4",
        "drupal/views_tree": "^2.0@alpha",
        "drupal/webform": "^6.2",
        "drupal/weight": "^3.2",
        "drupal/workbench_email": "^3.0",
        "drush/drush": "^12.4.4",
        "kartsims/easysvg": "2.5",
        "npm-asset/accessible-menu": "4.x-dev#2481e91",
