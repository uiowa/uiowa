        "drupal/metatag": "^2.0",
        "drupal/migrate_plus": "^6.0",
        "drupal/migrate_tools": "^6.0",
        "drupal/moderated_content_bulk_publish": "^3.0@beta",
        "drupal/node_revision_delete": "^2.0",
        "drupal/node_view_permissions": "^1.5",
        "drupal/override_node_options": "^2.4",
        "drupal/paragraphs": "^1.10",
        "drupal/pathauto": "^1.6",
        "drupal/photoswipe": "^4.0",
        "drupal/piwik_pro": "^1.3",
        "drupal/private_content": "^2.1",
        "drupal/purge": "^3.4",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/redirect": "^1.8",
        "drupal/replicate": "^1.0",
        "drupal/replicate_ui": "^1.0",
        "drupal/reroute_email": "^2.1",
        "drupal/responsive_table_filter": "^2.0",
        "drupal/robotstxt": "^1.2",
        "drupal/scheduler": "^2.0",
