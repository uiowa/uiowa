{
    "name": "uiowa/uiowa",
    "description": "SiteNow website platform by the University of Iowa",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "GPL-2.0-only",
    "repositories": {
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "blt-drupal-test": {
            "type": "git",
            "url": "https://github.com/modularlab/blt-drupal-test.git"
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "uiowa_auth": {
            "type": "git",
            "url": "git@github.com:uiowa/uiowa_auth"
        },
        "vanilla-icon-picker": {
            "type": "package",
            "package": {
                "name": "appolodev/vanilla-icon-picker",
                "version": "v1.1.3",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/AppoloDev/vanilla-icon-picker/archive/refs/tags/v1.1.3.zip",
                    "type": "zip"
                }
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Uiowa\\": "blt/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Uiowa\\Tests\\PHPUnit\\": "tests/phpunit/src/"
        }
    },
    "config": {
        "audit": {
            "abandoned": "ignore"
        },
        "sort-packages": true,
        "platform": {
            "php": "8.3"
        },
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "acquia/blt": true,
            "acquia/blt-phpcs": true,
            "drupal/core-composer-scaffold": true,
            "oomphinc/composer-installers-extender": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true,
            "tbachert/spi": true
        }
    },
    "require": {
        "ext-json": "*",
        "ext-simplexml": "*",
