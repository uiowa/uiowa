<?php

/**
 * @file
 * Install, update and uninstall functions for the Sitenow Search module.
 */

/**
 * Implements hook_install().
 */
function sitenow_search_install() {
  \Drupal::messenger()->addStatus(__FUNCTION__);
}

/**
 * Implements hook_uninstall().
 */
function sitenow_search_uninstall() {
  \Drupal::messenger()->addStatus(__FUNCTION__);
}
