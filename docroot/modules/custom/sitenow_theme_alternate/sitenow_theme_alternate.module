<?php

/**
 * @file
 * Primary module hooks for sitenow_theme_alternate module.
 */

/**
 * Implements hook_preprocess_HOOK().
 */
function sitenow_theme_alternate_preprocess_page(&$variables) {
  $variables['#attached']['library'][] = 'sitenow_theme_alternate/sitenow_theme_alternate';
  $variables['default_logo'] = \Drupal::config('uids_base.settings')->get('logo.use_default');
}
