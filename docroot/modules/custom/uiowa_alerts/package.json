{
  "name": "uiowa_alerts",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf assets",
    "sass-compile": "sass --silence-deprecation=import --no-source-map --style=compressed sass:assets/css",
    "sass-watch": "sass --watch --style=expanded sass:assets/css",
    "es6-compile": "babel es6 --out-dir assets/js --minified --no-comments",
    "es6-watch": "babel es6 --watch --out-dir assets/js --source-maps --verbose",
    "prebuild": "run-s clean",
    "build": "run-p es6-compile sass-compile",
    "watch": "run-p es6-watch sass-watch"
  },
  "dependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "sass": "^1.69.0",
    "npm-run-all": "^4.1.3",
    "uids_base": "^1.0.0"
  }
}
