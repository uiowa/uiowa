{
  "name": "uiowa_profiles",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf assets/css assets/js",
    "sass-compile": "sass --no-source-map --style=compressed sass:assets/css",
    "sass-watch": "sass --watch --style=compressed sass:assets/css",
    "prebuild": "run-s clean",
    "build": "run-p sass-compile",
    "watch": "run-p sass-watch"
  },
  "dependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "sass": "^1.83.4",
    "npm-run-all": "^4.1.3",
    "uids_base": "^1.0.0"
  }
}
