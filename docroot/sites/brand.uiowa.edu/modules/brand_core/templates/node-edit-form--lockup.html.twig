{#
/**
 * @file
 * Theme override for a node edit form.
 *
 * Two column template for the node add/edit form.
 *
 * This template will be used when a node edit form specifies 'node_edit_form'
 * as its #theme callback.  Otherwise, by default, node add/edit forms will be
 * themed by form.html.twig.
 *
 * Available variables:
 * - form: The node add/edit form.
 *
 * @see seven_form_node_form_alter()
 */
#}
<div class="layout-node-form clearfix">
  <div class="layout-region layout-region-node-main">
    <div class="lockup-preview-wrapper">
      <h2>Lockup Preview</h2>
      <div id="lockup-preview">
        <div class="lockup-horizontal">
          <img class="block-iowa" src="/{{ form['#module_path'] }}/images/iowa.svg">
          <div class="lockup-content">
            <div class="lockup-content-inner">
              <div class="primary-unit"></div>
              <div class="sub-unit"></div>
            </div>
          </div>
        </div>
        <div class="lockup-stacked">
          <img class="block-iowa" src="/{{ form['#module_path'] }}/images/iowa.svg">
          <div class="lockup-content">
            <div class="lockup-content-inner">
              <div class="primary-unit"></div>
              <div class="sub-unit"></div>
            </div>
          </div>

        </div>
      </div>
      <p><em>Disclaimer: This preview is for illustrative purposes only and is not an exact representation of final product.</em></p>
    </div>
    {{ form|without('advanced', 'footer', 'actions') }}
  </div>
  <div class="layout-region layout-region-node-secondary">
    {{ form.advanced }}
  </div>
  <div class="layout-region layout-region-node-footer">
    <div class="layout-region-node-footer__content">
      {{ form.footer }}
      {{ form.actions }}
    </div>
  </div>
</div>
