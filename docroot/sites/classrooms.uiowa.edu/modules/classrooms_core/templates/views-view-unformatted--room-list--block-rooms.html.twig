{% if title %}
  <h3>{{ title }}</h3>
{% endif %}
{% for row in rows %}
  {%
    set row_classes = [
    default_row_class ? 'views-row',
  ]
  %}

  {% embed '@uids_base/uids/card.html.twig' with row.card_options %}

    {% block card_img %}
    {% if card_image|striptags('<img><picture>')|trim is not empty %}
      {{ parent() }}
    {% endif %}
    {% endblock %}

  {% endembed %}
{% endfor %}
