{
  "name": "facilities_core",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf assets/js",
    "es6-compile": "babel es6 --out-dir assets/js --minified --no-comments",
    "es6-watch": "babel es6 --watch --out-dir assets/js --source-maps --verbose",
    "prebuild": "run-s clean",
    "build": "run-p es6-compile",
    "watch": "run-p es6-watch"
  },
  "dependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "node-sass": "^9.0.0",
    "node-sass-glob-importer": "^5.3.3",
    "npm-run-all": "^4.1.3",
    "uids_base": "^1.0.0"
  }
}
