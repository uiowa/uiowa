{
  "name": "facilities_core",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf assets/js",
    "js-compile": "babel js --out-dir assets/js --minified --no-comments",
    "js-watch": "babel js --watch --out-dir assets/js --source-maps --verbose",
    "prebuild": "run-s clean",
    "build": "run-p js-compile",
    "watch": "run-p js-watch"
  },
  "dependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "npm-run-all": "^4.1.3",
    "uids_base": "^1.0.0"
  }
}
