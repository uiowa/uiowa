{%
  set classes = [
  'card--stacked ',
  'card--centered-left',
  'card--alignment-left',
  'card--enclosed',
]
%}

{% set aos_card = {
  'attributes': attributes.addClass(classes),
  'card_image': content.field_image|render,
  'card_text': content.description|render,
  'card_title': name,
  'card_link_url': url,
} %}

{% embed '@uids_base/uids/card.html.twig' with aos_card only %}
{% endembed %}
