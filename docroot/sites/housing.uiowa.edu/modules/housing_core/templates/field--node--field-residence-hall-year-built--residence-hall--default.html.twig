{% embed '@uids_base/field/field.html.twig' %}
  {% block field_icon %}
    <span role="presentation" class="fa-calendar far"></span> &nbsp;
  {% endblock %}
  {% block field_items %}
    {% for item in items %}
      <{{ item_tag }}{{ item.attributes.addClass('field__item') }}>Opened in {{ item.content }}</{{ item_tag }}>
    {% endfor %}
  {% endblock %}
{% endembed %}
