{%
  set classes = [
  'card--stacked',
  'card--enclosed'
]
%}

{% set block_card = {
  'attributes': attributes.addClass(classes),
  'card_image' : content.field_image|render,
  'card_title': label,
  'card_link_url': url,
  'headline_level': headline_level,
  'visible_button': false,
} %}

{% embed '@uids_base/uids/card.html.twig' with block_card only %}{% endembed %}
