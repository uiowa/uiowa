{
  "name": "pharmacy_core",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf assets",
    "sass-compile": "sass --silence-deprecation=import --no-source-map --style=compressed sass:assets/css",
    "sass-watch": "sass --watch --style=compressed sass:assets/css",
    "prebuild": "run-s clean",
    "build": "run-p sass-compile",
    "watch": "run-p sass-watch"
  },
  "dependencies": {
    "sass": "^1.69.0",
    "npm-run-all": "^4.1.3",
    "uids_base": "^1.0.0"
  }
}
