<footer class="footer">
  <div class="footer__container">

    <div class="footer__links footer__links--bttn">

      <ul class="menu">
        {% for item in link_bttn.items | slice(0, 4) %}
          {% include "@footer--links-bttn" with {
            "link_bttn_url": item.link_bttn_url,
            "link_bttn_class": item.link_bttn_class,
            "link_bttn_icon": item.link_bttn_icon,
            "link_bttn_title": item.link_bttn_title,
          } %}
        {% endfor %}
      </ul>

    </div>
  </div>

  <div class="footer__container footer__container--flex">

    <div class="uiowa-footer--default">

      <a href="https://hr.uiowa.edu"><img class="footer__logo" src="{{ footer_logo }}" alt="Logo for University of Iowa">
      </a>

      <div class="block--additional-info">
        <p>Iowa City, Iowa 52242<br>
          319-335-3500</p>
      </div>

      <div class="footer__links footer__links--icon">
        <ul class="menu">
          {% for item in link_icon.items | slice(0, 5) %}
            {% include "@footer--links-icon" with {
              "link_icon_url": item.link_icon_url,
              "link_icon_class": item.link_icon_class,
              "link_icon_title": item.link_icon_title
            } %}
          {% endfor %}
        </ul>
      </div>
    </div>

    <div class="footer__links footer__links--nav">
      <nav role="navigation">

        <ul class="menu">
          {% for link in links_nav.links %}
            {% include "@footer--links-nav" with {
              "link_nav_url": item.link_nav_url,
              "link_nav_title": item.link_nav_title
            } %}
          {% endfor %}
        </ul>

      </nav>
    </div>

    <div class="footer__links footer__links--nav">
      <nav role="navigation">
        <ul class="menu">
          {% for link in links_nav.links %}
            {% include "@footer--links-nav" with {
              "link_nav_url": item.link_nav_url,
              "link_nav_title": item.link_nav_title
            } %}
          {% endfor %}
        </ul>
      </nav>
    </div>

  </div>
</footer>

